<html>
<head>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
</head>
<body>
<div class="content ">
 <div class="vertical-center">
        <img src="{{ asset('build/logo.png') }}">

    {{ form_start(form) }}
    <div class="input-button-group ">
        {% if password is defined and password is not null %}
        {{ form_widget(form.password,{'attr':{'disabled':'disabled','class':'password','value':password}}) }}
        {% else %}
            {{ form_widget(form.password,{'attr':{'disabled':'disabled','class':'password'}}) }}
        {% endif %}
        <button type="submit" class="fa-sync-alt">Generate</button>
    </div>
    {{ form_end(form) }}
 </div>
</div>
